<h2>All Users</h2>

<table class="table table-striped col-md-12" id="users">
  <thead>
    <tr>
      <th><%= @id_name %></th>
      <th>Name</th>
      <th>Email</th>
      <th>Date Joined</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td>
          <%= user.uid %>
        </td>
        <td>
          <%= user.name %>
        </td>
        <td>
          <%= user.email %>
        </td>
        <td>
          <%= user.date_joined %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>